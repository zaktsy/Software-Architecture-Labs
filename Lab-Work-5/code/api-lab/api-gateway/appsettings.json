{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Routes": [
    {
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        { "Host": "events-api", "Port": 5400 }
      ],
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE"],
      "UpstreamPathTemplate": "/{url}"
    }
  ]
}
